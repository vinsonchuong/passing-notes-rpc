{
  "name": "passing-notes-rpc",
  "version": "0.0.0",
  "description": "Simple communication between browser and server",
  "keywords": [],
  "homepage": "https://github.com/vinsonchuong/passing-notes-rpc",
  "bugs": "https://github.com/vinsonchuong/passing-notes-rpc/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/passing-notes-rpc",
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "type": "module",
  "devDependencies": {
    "ava": "^3.15.0",
    "ava-patterns": "^2.4.1",
    "passing-notes-ui": "^1.0.3",
    "puppet-strings": "^4.0.1",
    "puppet-strings-chrome": "^2.0.1",
    "quick-install": "^2.0.0",
    "semantic-release": "^17.4.1",
    "xo": "^0.38.2"
  },
  "ava": {
    "verbose": true,
    "timeout": "60s"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "rules": {
      "import/no-anonymous-default-export": "off"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  }
